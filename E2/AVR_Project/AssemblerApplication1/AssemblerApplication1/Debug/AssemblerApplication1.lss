000005 e0f0                      	ldi ZH, high(key<<1)	; load key into ST11-ST44
000006 e7e6                      	ldi ZL, low(key<<1)
000007 9005                      	lpm ST11, Z+
000008 9015                      	lpm ST21, Z+
000009 9025                      	lpm ST31, Z+
00000a 9035                      	lpm ST41, Z+
00000b 9045                      	lpm ST12, Z+
